@{
    ViewBag.Title = "Desktop";
    Layout = null;
}


<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html>
<head>
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/Site.css")?v=1.1" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.7.1.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-ui-1.8.20.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/swfupload/swfupload.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/swfupload/handlers.js")" type="text/javascript"></script>
    <script src="~/Content/Scripts/miniui/boot.js" type="text/javascript"></script>
    <script src="~/Content/Scripts/Script1.js" type="text/javascript"></script> 
    </head>
<body>
<div id="layout1" class="mini-layout" style="width:100%;height:400px;"  borderStyle="border:solid 0px #aaa;">
    <div  region="north" height="150" showHeader="false" >
        north
    </div>
    <div title="south" region="south" showSplit="false" showHeader="false" height="80"  style="border:none;" >
        south
    </div>
    <div title="功能菜单" region="west" width="200" showSplit="true" expanded="true">
        west
    </div>
   
    <div title="center" region="center"  style="border:none;"  >
       <iframe onload="onIFrameLoad()" src="overview.html" id="mainframe" frameborder="0" name="main" style="width:100%;height:100%;" border="0"></iframe>
    </div>
</div>
        

</body>
</html>